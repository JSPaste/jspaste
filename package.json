{
  "name": "jspaste",
  "version": "0.0.0",
  "description": "The official library to interact with JSPaste API",
  "keywords": [
    "hastebin",
    "jspaste",
    "paste",
    "pastebin",
    "tnfangel"
  ],
  "homepage": "https://jspaste.tnfangel.xyz",
  "bugs": {
    "url": "https://github.com/jspaste/jspaste/issues"
  },
  "repository": "jspaste/jspaste",
  "license": "MIT",
  "author": "tnfAngel",
  "contributors": [
    "Aidak",
    "Ivan Gabaldon <git[at]inetol.net>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "bun": "./dist/index.mjs",
      "default": "./dist/index.mjs",
      "deno": "./dist/index.mjs",
      "node": {
        "import": {
          "default": "./dist/index.mjs",
          "types": "./types/index.d.mts"
        },
        "require": {
          "default": "./dist/index.cjs",
          "types": "./types/index.d.cts"
        }
      },
      "types": "./types/index.d.mts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./types/index.d.cts",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "tsc && esbuild --bundle --minify --platform=node --outfile=dist/index.cjs src/index.ts && esbuild --bundle --minify --platform=neutral --outfile=dist/index.mjs src/index.ts",
    "lint": "ts-standard --fix",
    "prepack": "bun run build",
    "test": "bun run lint && bun test",
    "test:node": "bun run lint && NODE_OPTIONS=--experimental-vm-modules npm exec jest"
  },
  "dependencies": {
    "node-fetch-native": "^1.4.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "bun-types": "*",
    "esbuild": "^0.19.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-standard": "*",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.11.0"
  },
  "engineStrict": true,
  "ts-standard": {
    "ignore": [
      "__coverage__",
      "dist"
    ],
    "project": "tsconfig.json"
  }
}
