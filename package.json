{
  "name": "jspaste",
  "version": "10.0.0",
  "description": "Powerful library to easily interact with JSPaste API",
  "license": "BSD-3-Clause-Clear",
  "author": "tnfAngel",
  "contributors": [
    "Aidak",
    "Ivan Gabaldon <ivan [d0t] gab [at] inetol [d0t] net>"
  ],
  "homepage": "https://jspaste.ml",
  "repository": "github:tnfAngel/jspaste-api",
  "bugs": {
    "url": "https://github.com/tnfAngel/jspaste-api/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:check && tsup-node",
    "build:check": "tsc",
    "prepublishOnly": "npm run test && npm run build",
    "test": "npm run build:check && jest --no-cache"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "centra": "^2.6.0"
  },
  "devDependencies": {
    "@types/centra": "^2.2.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.11",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  },
  "keywords": [
    "hastebin",
    "jspaste",
    "pastebin",
    "tnfangel"
  ],
  "engines": {
    "node": ">=16.13.0",
    "pnpm": "use-npm-instead",
    "yarn": "use-npm-instead"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.ts"
    ],
    "coverageReporters": [
      "json"
    ],
    "errorOnDeprecated": true,
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/test/"
    ],
    "testTimeout": 60000
  },
  "tsup": {
    "platform": "node",
    "target": "node16",
    "entry": [
      "lib/index.ts"
    ],
    "format": "esm",
    "minify": true,
    "dts": true,
    "splitting": false,
    "clean": true
  }
}
